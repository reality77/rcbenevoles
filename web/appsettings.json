{
  "Serilog": {
    "Using":  ["Serilog.Sinks.Literate"],
    "MinimumLevel": {
      "Default" : "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "LiterateConsole" },
      { "Name": "File", "Args": { "path": "/logs/log.txt", "rollingInterval": "Day" } }
    ],
    "Enrich": ["FromLogContext", "WithThreadId"],
    "Properties": {
		"Application": "rcbenevoles"
    }
  }
}